{
  "trailingSlash": "auto",
  "routes": [
    {
      "route": "/api/ticket",
      "allowedRoles": ["reader"],
      "headers": {
        "Cache-Control": "no-store"
    }
    },
    {
      "route": "/index2.html",
      "allowedRoles": ["writer"],
      "headers": {
        "Cache-Control": "no-store"
    }
    },
    {
      "route": "/api/check",
      "allowedRoles": ["anonymous"]
      
    }

  ],
  "responseOverrides": {
    "401": {
      "statusCode": 302,
      "redirect": "/.auth/login/aad"
    }
  },
  "responseOverrides": {
    "402": {
      "statusCode": 403,
      "redirect": "https://ccinfralab.com"
    }
  },
  "networking": {
    "allowedIpRanges": ["AzureFrontDoor.Backend"]
  }
}